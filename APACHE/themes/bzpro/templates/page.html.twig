{% include  "@bzpro/header.html.twig" %}
{% set description = node.field_description_basic_home.value %}
	<div id="home" class="bg-inner low-back-gradient-inner">
	  <div class="text-con-inner low-back-up">
	    <div class="container">
	      <div class="row">
	        <div class="lead col-lg-12 col-xm-12 text-center">
	          <h1><span class="top-heading-inner">{{ title }}</span></h1>
	          <div class="list-o-i white">
	            <p class="white no-p">{{ description|raw }}</p>
	          </div>
	        </div>
	      </div>
	    </div>
	  </div>
	</div>

	<!--Pagenation -->
	<div class="pagenation-holder">
	  <div class="container">
	    <div class="row">
	      <div class="col-md-6 text-left">
	        <div class="pagenation_links">{{ page.breadcrumb }}</div>
	      </div>
	      <div class="col-md-6"> </div>
	    </div>
	  </div>
	</div>
	<!--END Pagenation --> 

{% if page.content %}
<div class="container" style="padding: 30px;">
	{{ page.content }}
</div>
    
{% endif %}

{% if page.section %}
    {{ page.section }}
{% endif %}

    
{% include "@bzpro/footer.html.twig" %}
